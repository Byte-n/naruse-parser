{
  "scripts": {
    "start": "rollup -c ./rollup.config.js -w",
    "build": "rollup -c ./rollup.config.js && mocha ./test/*",
    "test": "mocha ./test/*"
  },
  "name": "naruse-parser",
  "description": "只有18kb的一个js编译器+js解释器",
  "version": "0.0.9",
  "repository": {
    "type": "git",
    "url": "https://github.com/narusejs/naruse-parser"
  },
  "type": "module",
  "main": "dist/dev.cjs.js",
  "module": "dist/dev.esm.js",
  "exports": {
    "import": "./dist/dev.esm.js",
    "require": "./dist/dev.cjs.js"
  },
  "directories": {
    "test": "test"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-alias": "4.0.2",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@rollup/plugin-typescript": "9.0.2",
    "@types/estree": "^1.0.0",
    "chai": "^4.3.6",
    "chai-properties": "1.4.0",
    "mocha": "^10.0.0",
    "rollup": "3.2.5",
    "rollup-plugin-dts": "^5.0.0",
    "rollup-plugin-uglify": "^6.0.4",
    "tslib": "^2.4.0",
    "typescript": "^4.8.3"
  },
  "keywords": [
    "js",
    "interpreter",
    "js compiler"
  ]
